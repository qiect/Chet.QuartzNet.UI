{
  "name": "Chet.QuartzNet.UI",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "chet",
    "quartz net",
    "quartz net ui",
    "vben admin",
    "vue",
    "vue admin",
    "vue vben admin",
    "vue vben admin pro",
    "vue3",
    "net",
    "net 8"
  ],
  "homepage": "https://github.com/qiect/Chet.QuartzNet.UI",
  "bugs": "https://github.com/qiect/Chet.QuartzNet.UI/issues",
  "repository": "qiect/Chet.QuartzNet.UI.git",
  "license": "MIT",
  "author": {
    "name": "Chet",
    "email": "qctvip@vip.163.com",
    "url": "https://github.com/qiect"
  },
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:analyze": "turbo build:analyze",
    "build:antd": "pnpm run build --filter=@chet/chet.quartznet.ui",
    "changeset": "pnpm exec changeset",
    "check": "pnpm run check:circular && pnpm run check:dep && pnpm run check:type && pnpm check:cspell",
    "check:circular": "vsh check-circular",
    "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress",
    "check:dep": "vsh check-dep",
    "check:type": "turbo run typecheck",
    "clean": "node ./scripts/clean.mjs",
    "postinstall": "pnpm -r run stub --if-present",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vben/tailwind-config": "workspace:*",
    "@vben/tsconfig": "workspace:*",
    "@vben/turbo-run": "workspace:*",
    "@vben/vite-config": "workspace:*",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "cross-env": "catalog:",
    "rimraf": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.22.0",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "overrides": {
      "@ast-grep/napi": "catalog:",
      "@ctrl/tinycolor": "catalog:",
      "clsx": "catalog:",
      "esbuild": "0.25.3",
      "jiti": "catalog:",
      "pinia": "catalog:",
      "vue": "catalog:"
    },
    "neverBuiltDependencies": [
      "canvas",
      "node-gyp"
    ]
  }
}
